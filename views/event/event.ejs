<script>

function copytoken(e) {
    copyclipboard(e);
    $('#notif').modal({keyboard: true });
    $('.modal-backdrop').removeClass("modal-backdrop");

    setTimeout(()=>{
        $('#notif').modal('hide');
    }, 1000);
}
function copyclipboard(text) {var copytxt = document.createElement("textarea");document.body.appendChild(copytxt);copytxt.value = text;copytxt.select();document.execCommand('copy');document.body.removeChild(copytxt);}

</script>
