        </ul>
      </nav>
      <!-- /.sidebar-menu -->
    </div>
    <!-- /.sidebar -->
  </aside>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>GENERATE TICKETS</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">Generate tickets</li>
             
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
            <div class="card card-default">
                    <div class="card-header">
                      <h3 class="card-title">Event ticket generation</h3>
          
                      <div class="card-tools">
                        <button type="button" class="btn btn-tool" data-card-widget="collapse"><i class="fas fa-minus"></i></button>
                        <button type="button" class="btn btn-tool" data-card-widget="remove"><i class="fas fa-remove"></i></button>
                      </div>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body" style="display: block;">
                      <div class="row">
                        <div class="col-md-6">
                          <div class="form-group">
                            <label>Event ID</label>
                            <div class="bootstrap-duallistbox-container row moveonselect moveondoubleclick"> 
                                <input class="form-control filter" type="text" placeholder="enter id" id="searchticket" >
                                    <select multiple="multiple" id="ft"style="height: 102px;">
  
                                    </select>
                                 
                          </div>
                          <!-- /.form-group -->
                        </div>
                        <!-- /.col -->
                      </div>
                      <div class=" col-md-6">
                            <div class="card-body">
                                <div class="form-group">
                                    <label for="exampleInputEmail1">Unique id</label>
                                    <i class="uid_err text-danger"></i>
                                    <input type="email" class="form-control" id="uid" disabled>
                                </div>
                                <div class="form-group">
                                        <label>Ticket Type</label>
                                        <select class="custom-select" id="ttype">
                                            <option value="pin">pin</option>
                                            <option value="qrcode">qrcode</option>
                                        </select>
                                    </div>
                                <div class="form-group">
                                    <label for="exampleInputFile">Number of ticket</label>
                                    <i class="lmt_err text-danger"></i>
                                    <input type="text" class="form-control" placeholder="enter number of ticket" id="lmt">
                                </div>
                                <div class="form-group">
                                    <button id="generateticket"> Generate</button>
                                </div> 
                            </div>
                    </div>
                      <!-- /.row -->
                    </div>
                  </div>
    </section>
    <!-- /.content -->
