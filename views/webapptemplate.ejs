<script>
$('#_v').on('click',()=>{
    let p = $('#_p').val();
    
    $.ajax({
      type        : 'post',
      data        : {p:p,_csrf:$('meta[name="csrf-token"]').attr('content')},
      dataType    : 'json',
      url         : '/auth/temp',
      success     : function(data){
          if(data.status == 4){
              $('.err_l').text(data.msg)
          }else if(data.status == 3){
            swal({
               title: data.msg,
               icon: "error",
               button: 'close',
               dangerMode: true,
               })
          }else if(data.status == 2){
            swal({
               title: data.msg,
               icon: "warning",
               button: 'close',
               dangerMode: true,
               })
          }else if(data.status == 1){
            swal(data.msg, "", "success");
          }
          
      }
    });
    
})
</script>
